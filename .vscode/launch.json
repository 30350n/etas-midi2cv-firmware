{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "rust: cargo build",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "rust: probe-rs debug",
            "cwd": "${workspaceFolder}",
            "programBinary": "${workspaceFolder}/target/thumbv7m-none-eabi/debug/${workspaceFolderBasename}",
            "chip": "STM32F103C8",
            "runtimeExecutable": "probe-rs-debugger",
            "connectUnderReset": false,
            "flashingEnabled": true,
            "resetAfterFlashing": true,
            "haltAfterReset": false,
            "rttEnabled": true,
        }
    ]
}